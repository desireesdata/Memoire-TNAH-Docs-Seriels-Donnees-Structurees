@inproceedings{chiron,
	address = {Grenoble, France},
	title = {Erreurs {OCR} et biais d'indexation : impact sur les usages},
	shorttitle = {Erreurs {OCR} et biais d'indexation},
	url = {https://hal.science/hal-01455763},
	abstract = {Les méthodes d'analyse classiquement appliquées dans le contexte du Big Data, provoquent souvent un phénomène de « boîte noire » où la qualité de numérisation des documents peut être un paramètre négligé. En dépit des bonnes pratiques en vigueur inhérentes au métier de data-journalist, se pose la problématique des biais statistiques induits par ce manque de transparence sur la fiabilité des sources. S'inscrivant dans le cadre du projet AméliOCR, cet article vise à estimer ces potentiels biais sur l'indexation et la recherche. Cette étude s'appuie sur un corpus de documents OCéRisés associés à leur vérité terrain, ainsi que sur des historiques de recherche sur Gallica.},
	urldate = {2025-08-22},
	booktitle = {17ème conférence {Extraction} et {Gestion} des {Connaissances}, {Atelier} {Journalisme} {Computationnel}},
	author = {Chiron, Guillaume and Moreux, Jean-Philippe and Doucet, Antoine and Coustaty, Mickaël and Visani, Muriel},
	month = jan,
	year = {2017},
	keywords = {Numérisation},
	pages = {69--73},
	file = {HAL PDF Full Text:/home/joel/snap/zotero-snap/common/Zotero/storage/FSBNJGFK/Chiron et al. - 2017 - Erreurs OCR et biais d'indexation  impact sur les usages.pdf:application/pdf},
