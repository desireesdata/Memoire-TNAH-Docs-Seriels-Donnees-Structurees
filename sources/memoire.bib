	keywords = {Numérisation},
	pages = {13--18},
	file = {PDF:/home/joel/snap/zotero-snap/common/Zotero/storage/D55BIWZH/Neudecker et al. - 2021 - A survey of OCR evaluation tools and metrics.pdf:application/pdf},
